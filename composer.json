{
    "name" : "olix/formsext-bootstrap-bundle",
    "description" : "Bootstrap forms extended into Symfony2",
    "type" : "symfony-bundle",
    "homepage" : "https://github.com/sabinus52/OlixFormsExtBootstrapBundle",
    "license" : "GPL-2.0",
    "authors" : [{
            "name" : "Olivier",
            "email" : "sabinus52@gmail.com"
        }
    ],
    "keywords" : [
        "olix",
        "symfony",
        "admin",
        "back office",
        "bootstrap",
        "forms"
    ],
    "require": {
        "symfony/symfony": "~2.8",
        "symfony/form": "~2.8",
        "components/bootstrap": "3.2.*",
        "nostalgiaz/bootstrap-switch": "3.2.*",
        "components/bootstrap-datetimepicker": "2.*",
        "ivaynberg/select2": "3.5.*",
        "components/multi-select": "0.9.*",
        "twitter/typeahead.js": "0.11.*"
    },
    "autoload" : {
        "psr-4" : {
            "Olix\\FormsExtBootstrapBundle\\" : ""
        }
    },
    "extra": {
        "component": {
            "twitter/typeahead.js": {
                "scripts": [
                    "dist/typeahead.jquery.js"
                ],
                "files": [
                    "dist/*.js"
                ]
            }
        }
    }
}
