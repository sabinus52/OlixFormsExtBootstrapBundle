{###}
{# Blocks JAVASCRIPT des formulaires #}
{##}

{% block form_javascript %}
    {% for child in form %}
        {{ form_javascript(child) }}
    {% endfor %}
{% endblock %}


{% block olix_switch_javascript %}
{% spaceless %}
    <script type="text/javascript">
        jQuery(document).ready(function($) {
            $('#{{ id }}').bootstrapSwitch();
        });
    </script>
{% endspaceless %}
{% endblock olix_switch_javascript %}

